{
    "manifest_version": 2,

    "name": "AI Assistant",
    "version": "1.0",

    "chrome_url_overrides":{
        "newtab":"index.html"
    },

    "browser_action":
    {
      "default_icon": "icon.jpg"
    },

    "background":
    {
      "scripts":["background.js"], 
      "persistent":false
    },
    "permissions": ["tabs"]
    
  }